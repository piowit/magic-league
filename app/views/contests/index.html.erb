<h1>Contests</h1>

  <table class='table'>
    <thead class="table-dark">
      <tr>
        <th>Date</th>
        <th>League</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% contests.each do |contest| %>
        <tr>
          <td><%= contest.date %></td>
          <td><%= contest.league.name %></td>
          <td><%= link_to "Show", contest %></td>
        </tr>
      <% end %>
    </tbody>
</table>
